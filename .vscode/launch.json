    "version": "0.2.0",
    "configurations": [
        {
            "name": "Unicorn: app.main:app --reload",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
            "app.main:app",
            "--reload"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "env": {
                "APP_ENV": "development"
            }
        }
    ]
